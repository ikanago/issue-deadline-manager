{
  "name": "issue-deadline-manager",
  "version": "0.0.0",
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "check": "npm run format-check && npm run lint",
    "build": "npx tsc --build",
    "build:package": "ncc build --source-map --license licenses.txt",
    "build:all": "npm run build && npm run build:package",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "author": "ikanago",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@octokit/core": "^3.5.1",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.1.6"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@typescript-eslint/parser": "4.33.0",
    "@vercel/ncc": "0.31.1",
    "eslint": "7.32.0",
    "eslint-plugin-github": "4.3.2",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "js-yaml": "4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  }
}
